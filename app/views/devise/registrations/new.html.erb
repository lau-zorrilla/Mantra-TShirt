<div id="newAdminContainer">
<h1>Sign up</h1>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div id="adminEmail"><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div id="adminPwd"><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div id="adminConfirmation"><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

   <div id="newAdminLinks"><%= f.submit "Sign up" %></div>
<% end %>

<p><%= render "devise/shared/links" %></p>
